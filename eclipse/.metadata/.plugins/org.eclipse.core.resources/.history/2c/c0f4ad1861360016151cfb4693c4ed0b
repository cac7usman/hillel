package hw_02_02_loops;

import static org.junit.Assert.assertFalse;
import static org.junit.Assert.assertTrue;

import org.junit.Test;

public class HW_02_02_02Test
{

	@Test
	public void testIsPrime_01() throws Exception
	{

		assertTrue(HW_02_02_02.isPrime(5));
		assertFalse(HW_02_02_02.isPrime(6));
	}

	@Test
	public void testIsPrime_02() throws Exception
	{

		assertTrue(HW_02_02_02.isPrime(-5));
		assertFalse(HW_02_02_02.isPrime(-6));
	}

}
