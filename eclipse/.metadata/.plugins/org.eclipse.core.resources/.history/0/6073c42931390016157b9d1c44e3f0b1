package hw_02_03_arrays;

import java.util.Arrays;

/*
 * Найти максимальный элемент массива
 */
public class HW_02_03_02_Max
{

	public static int getMax(int[] array)
	{

		for (int i = 0; i < array.length; i++)
		{
			if (array[i] > array[0])
				array[0] = array[i];
		}
		return array[0];
	}
}
