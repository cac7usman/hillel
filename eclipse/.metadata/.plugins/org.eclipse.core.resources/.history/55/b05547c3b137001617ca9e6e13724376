package hw_03_stars;

public class HW_03_03
{
	public static void main(String[] args)
	{
		print();
	}

	private static void print()
	{
		int row = 7;
		int column = 7;
		for (int j = 0; j < column; j++)
		{
			for (int i = 0; i < row; i++)
			{
				System.out.print("*");
				row--;
			}
			System.out.println();
			column--;
		}
	}
}