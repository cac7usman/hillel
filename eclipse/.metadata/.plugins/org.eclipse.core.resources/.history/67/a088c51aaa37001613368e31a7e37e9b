package hw_03_stars;

public class HW_03_11
{

	public static void main(String[] args)
	{
		print();
	}

	private static void print()
	{
		int n = 1;
		for (int j = 0; j < 7; j++)
		{
			for (int i = 0; i < 7; i++)
			{
				System.out.print(n);
			}
			System.out.println("");
			n++;
		}
	}
}
