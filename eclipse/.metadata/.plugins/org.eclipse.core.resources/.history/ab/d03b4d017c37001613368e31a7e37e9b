package hw_03_stars;

public class hw_03_02
{
	public static void main(String[] args)
	{
		print();
	}

	public static void print()
	{
		for (int j = 0; j < 7; j++)
		{
			for (int i = 0; i < 7; i++)
			{

				if (i == 0 || i == 6)
				{
					System.out.print("*");
				} else
				{
					System.out.print(" ");
				}
			}
			System.out.println();
		}
	}
}
