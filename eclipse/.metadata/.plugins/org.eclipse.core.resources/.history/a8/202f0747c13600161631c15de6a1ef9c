package hw_02_03_arrays;

import static org.junit.Assert.*;

import org.junit.Test;

public class HW_02_03_04Test
{

	@Test
	public void testGetMaxIndex()
	{
		int[] array =
			{ 10, 20, 30, 40, -50 };

			int test = HW_02_03_04.getMaxIndex(array);
			assertEquals(3, test);
	}

}
