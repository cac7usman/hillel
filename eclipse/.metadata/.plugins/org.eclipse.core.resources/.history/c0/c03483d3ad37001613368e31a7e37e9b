package hw_03_stars;

public class HW_03_03
{
	public static void main(String[] args)
	{
		print();
	}

	private static void print()
	{
		int column = 7;
		int row = 7;

		label: for (int i = 0; i < 10; i++)
		{
			for (int j = 0; j < 10; j++)
			{
				if (j > i)
				{

					System.out.println();

					continue label;
				}

				System.out.print("S");
			}

		}

	}
}
